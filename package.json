{
  "name": "management-for-special-disease",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve -p 4203 --sourcemaps false --progress false",
    "serve-pci": "ng serve -p 4201 -e pci-test",
    "serve-kidney": "ng serve -p 4202 -e kidney-test",
    "clean": "rm -rf dist && rm -rf management-pci && rm -rf management-kidney",
    "to-pci": "mkdir management-pci && cp -r dist/** management-pci",
    "to-kidney": "mkdir management-kidney && cp -r dist/** management-kidney",
    "to-all": "mkdir management && cp dist/** management",
    "build-test-pci": "npm run clean && ng build -aot -prod -e pci-test -bh /management-pci/ && npm run to-pci",
    "build-test-kidney": "npm run clean && ng build -aot -prod -e pci-test -bh /management-kidney/ && npm run to-kidney",
    "build-test": "npm run clean && ng build -aot -prod -e pci-test -bh /management/ && npm run to-all",
    "build-pci": "npm run clean && ng build -aot -prod -e pci-test -bh /management-pci/ && npm run to-pci",
    "build-kidney": "npm run clean && ng build -aot -prod -e pci-test -bh /management-kidney/ && npm run to-kidney",
    "build": "npm run clean && ng build -aot -prod -e pci-test -bh /management/ && npm run to-all"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/store": "^6.5.6",
    "@angular/animations": "^4.2.6",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.2.6",
    "@angular/compiler": "^4.2.6",
    "@angular/core": "^4.2.6",
    "@angular/forms": "^4.2.6",
    "@angular/http": "^4.2.6",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.2.6",
    "@angular/platform-browser-dynamic": "^4.2.6",
    "@angular/router": "^4.2.6",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.3",
    "flatpickr": "^3.0.6",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "material-design-icons": "^3.0.1",
    "moment": "^2.18.1",
    "ng-table-excel-export": "^1.0.0",
    "ngx-quill-editor": "^2.0.2",
    "redux": "^3.5.0",
    "redux-logger": "^3.0.6",
    "rxjs": "^5.4.2",
    "web-animations-js": "^2.2.5",
    "xlsx": "^0.11.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.1",
    "@angular/compiler-cli": "^4.2.6",
    "@angular/language-service": "^4.2.6",
    "@types/jasmine": "^2.5.45",
    "@types/node": "^6.0.60",
    "@types/quill": "0.0.31",
    "codelyzer": "^3.0.1",
    "jasmine-core": "^2.6.2",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.3",
    "protractor": "^5.1.2",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.3.3"
  }
}
