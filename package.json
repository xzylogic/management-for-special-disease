{
  "name": "management-for-special-disease",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve -p 4203",
    "serve-pci": "ng serve -p 4201 -e pci-test",
    "serve-kidney": "ng serve -p 4202 -e kidney-test",
    "clean": "rm -rf dist",
    "to-pci": "mkdir management-pci && cp -r dist/** management-pci",
    "to-kidney": "mkdir management-kidney && cp -r dist/** management-kidney",
    "to-all": "mkdir management && cp dist/** management",
    "build-test-pci": "npm run clean && ng build -aot -prod -e pci-test -bh /management-pci/ && npm run to-pci",
    "build-test-kidney": "npm run clean && ng build -aot -prod -e pci-test -bh /management-kidney/ && npm run to-kidney",
    "build-test": "npm run clean && ng build -aot -prod -e pci-test -bh /management/ && npm run to-all",
    "build-pci": "npm run clean && ng build -aot -prod -e pci-test -bh /management-pci/ && npm run to-pci",
    "build-kidney": "npm run clean && ng build -aot -prod -e pci-test -bh /management-kidney/ && npm run to-kidney",
    "build": "npm run clean && ng build -aot -prod -e pci-test -bh /management/ && npm run to-all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "jquery": "^3.2.1",
    "material-design-icons": "^3.0.1",
    "ng-table-excel-export": "^1.0.0",
    "rxjs": "^5.1.0",
    "swiper": "^3.4.2",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.1.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.3",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
